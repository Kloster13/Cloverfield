<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="550.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cloverfield.presentation.controllers.ManageTaskController">
    <children>
        <VBox prefHeight="550.0" prefWidth="650.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="564.0" text="All Planets" textAlignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                    <padding>
                        <Insets bottom="10.0" top="10.0" />
                    </padding>
                </Label>
                <Separator prefWidth="200.0" />
                <VBox alignment="TOP_CENTER" prefWidth="100.0" spacing="6.0">
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" top="10.0" />
                    </VBox.margin>
                    <children>
                        <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
                            <columns>
                                <TableColumn fx:id="nameTable" prefWidth="96.0" text="Name" />
                                <TableColumn fx:id="climateTable" prefWidth="69.0" text="Climate" />
                                <TableColumn fx:id="distanceTable" text="Distance" />
                                <TableColumn fx:id="atmosTable" prefWidth="82.0" text="Atmos." />
                                <TableColumn fx:id="lifeTable" prefWidth="84.0" text="Life" />
                            </columns>
                        </TableView>
                        <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#onBack" text="Back">
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
